    "#Q2\n",
    "\n",
    "Select distinct W.WORKER_ID, W.FIRST_NAME, W.Salary from Worker W, Worker W1\n",
    "where W.Salary = W1.Salary\n",
    "and W.WORKER_ID != W1.WORKER_ID;"

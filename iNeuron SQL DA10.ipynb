    "#Q1\n",
    "\n",
    "SELECT Salary\n",
    "FROM Worker W1\n",
    "WHERE 4 = (\n",
    "SELECT COUNT( DISTINCT ( W2.Salary ) )\n",
    "FROM Worker W2\n",
    "WHERE W2.Salary >= W1.Salary );"
   ]
  },
